a = []
count=0
for i in range(25):
    a.append(int(input()))
max=a[0]
e=0
while count < 25:
    if a[count]>max:
        max=a[count]
        e=count
        count+=1
    else :
        count+=1  
a=a[:e]+a[e+1:]
count=0
max=a[0]
while count < 24:
    if a[count]>max:
        max=a[count]
        count+=1
    else :
        count+=1   
print(max)
